<?xml version="1.0" encoding="UTF-8"?>
<!-- IAR Project Connection File -->

<iarProjectConnection version="1.2" name="UART Echo DMA" oneShot="true"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="IarProjectConnection_1.2.xsd">

  <device>
    <name>CC3220SF</name>
  </device>

  <extraOptions>
    <args>
      <arg>-I$PROJ_DIR$</arg>
      <arg>-DTI_DRIVERS_UART_DMA</arg>
      <arg>-I$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source</arg>
      <arg>-I$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/kernel/freertos/posix</arg>
      <arg>-I$FREERTOS_INSTALL_DIR$/FreeRTOS/Source/include</arg>
      <arg>-I$FREERTOS_INSTALL_DIR$/FreeRTOS/Source/portable/IAR/ARM_CM3</arg>
      <arg>-I$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/kernel/freertos/builds/CC3220SF_LAUNCHXL/release</arg>
      <arg>--debug</arg>
      <arg>--silent</arg>
      <arg>-e</arg>
      <arg>--aeabi</arg>
      <arg>--thumb</arg>
      <arg>--diag_suppress=Pa050</arg>
    </args>
  </extraOptions>

  <linkerFile>
    <override>true</override>
    <path>$PROJ_DIR$/$PROJ_FNAME$_CC3220SF_LAUNCHXL_FREERTOS.icf</path>
  </linkerFile>

  <linkerExtraOptions>
    <args>
      <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/display/lib/display.arm4</arg>
      <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/drivers/lib/drivers_cc32xx.arm4</arg>
      <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/third_party/fatfs/lib/fatfs.arm4</arg>
      <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/kernel/freertos/builds/CC3220SF_LAUNCHXL/release/iar/freertos.lib</arg>
      <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/devices/cc32xx/driverlib/iar/Release/driverlib.a</arg>
      <arg>--silent</arg>
      <arg>--cpu=Cortex-M4</arg>
      <arg>--redirect</arg>
      <arg>_Printf=_PrintfSmall</arg>
      <arg>--redirect</arg>
      <arg>_Scanf=_ScanfSmall</arg>
    </args>
  </linkerExtraOptions>

  <heaps>
    <heap> <!-- If omitted, id="0" -->
      <size>0</size>
    </heap>
    <heap id="1">
      <size>0</size>
    </heap>
  </heaps>
  <stacks>
    <stack> <!-- If omitted, id="0" -->
      <size>512</size>
    </stack>
  </stacks>

  <preBuild>
    <command>iarbuild.exe $SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/kernel/freertos/builds/CC3220SF_LAUNCHXL/release/iar/freertos_builds_CC3220SF_LAUNCHXL_release.ewp Debug</command>
  </preBuild>

  <postBuild>
    <command>ielftool.exe --bin --verbose "$PROJ_DIR$/Debug/Exe/$PROJ_FNAME$.out" "$PROJ_DIR$/Debug/Exe/$PROJ_FNAME$.bin"</command>
  </postBuild>

  <files>
    <group name="Source files">
      <path copyTo="$PROJ_DIR$/main_freertos.c">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/drivers/uartechodma/freertos/main_freertos.c</path>
      <path copyTo="$PROJ_DIR$/uartecho.c">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/drivers/uartechodma/uartecho.c</path>
      <path copyTo="$PROJ_DIR$/CC3220SF_LAUNCHXL.c">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/drivers/uartechodma/CC3220SF_LAUNCHXL.c</path>
      <path copyTo="$PROJ_DIR$/$PROJ_FNAME$_CC3220SF_LAUNCHXL_FREERTOS.icf">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/drivers/uartechodma/freertos/iar/CC3220SF_LAUNCHXL_FREERTOS.icf</path>
    </group>

    <group name="Header files">
      <path copyTo="$PROJ_DIR$/Board.h">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/drivers/uartechodma/Board.h</path>
      <path copyTo="$PROJ_DIR$/CC3220SF_LAUNCHXL.h">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/drivers/uartechodma/CC3220SF_LAUNCHXL.h</path>
    </group>

    <group name="Documentation" build="false">
      <path copyTo="$PROJ_DIR$/README.md">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/drivers/uartechodma/README.md</path>
      <path copyTo="$PROJ_DIR$/README.html">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/drivers/uartechodma/README.html</path>
      <path reference="$PROJ_DIR$/Board.html">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/boards/CC3220SF_LAUNCHXL/Board.html</path>
    </group>
  </files>
</iarProjectConnection>
