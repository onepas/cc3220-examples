<?xml version="1.0" encoding="UTF-8"?>
<!-- IAR Project Connection File -->

<iarProjectConnection version="1.2" name="Trigger Mode" oneShot="true"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="IarProjectConnection_1.2.xsd">

  <device>
    <name>CC3220S</name>
  </device>

  <extraOptions>
    <args>
      <arg>-I$PROJ_DIR$</arg>
      <arg>-I$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source</arg>
      <arg>-DNORTOS_SUPPORT</arg>
      <arg>-I$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/kernel/nortos</arg>
      <arg>-I$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/kernel/nortos/posix</arg>
      <arg>--debug</arg>
      <arg>--silent</arg>
      <arg>-e</arg>
      <arg>--aeabi</arg>
      <arg>--thumb</arg>
      <arg>--diag_suppress=Pa050</arg>
    </args>
  </extraOptions>

  <linkerFile>
    <override>true</override>
    <path>$PROJ_DIR$/$PROJ_FNAME$_CC3220S_LAUNCHXL_NoRTOS.icf</path>
  </linkerFile>

  <linkerExtraOptions>
    <args>
      <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/display/lib/display.arm4</arg>
      <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/drivers/lib/drivers_cc32xx.arm4</arg>
      <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/third_party/fatfs/lib/fatfs.arm4</arg>
      <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/drivers/net/wifi/iar/nortos/simplelink.a</arg>
      <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/kernel/nortos/lib/nortos_cc32xx.arm4</arg>
      <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/devices/cc32xx/driverlib/iar/Release/driverlib.a</arg>
      <arg>--silent</arg>
      <arg>--cpu=Cortex-M4</arg>
      <arg>--redirect</arg>
      <arg>_Printf=_PrintfSmall</arg>
      <arg>--redirect</arg>
      <arg>_Scanf=_ScanfSmall</arg>
    </args>
  </linkerExtraOptions>

  <heaps>
    <heap> <!-- If omitted, id="0" -->
      <size>0</size>
    </heap>
    <heap id="1">
      <size>0</size>
    </heap>
  </heaps>
  <stacks>
    <stack> <!-- If omitted, id="0" -->
      <size>512</size>
    </stack>
  </stacks>


  <postBuild>
    <command>ielftool.exe --bin --verbose "$PROJ_DIR$/Debug/Exe/$PROJ_FNAME$.out" "$PROJ_DIR$/Debug/Exe/$PROJ_FNAME$.bin"</command>
  </postBuild>

  <files>
    <group name="Source files">
      <path copyTo="$PROJ_DIR$/uart_term.c">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/nortos/CC3220S_LAUNCHXL/demos/trigger_mode/uart_term.c</path>
      <path copyTo="$PROJ_DIR$/main_nortos.c">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/nortos/CC3220S_LAUNCHXL/demos/trigger_mode/main_nortos.c</path>
      <path copyTo="$PROJ_DIR$/mqueue_nortos.c">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/nortos/CC3220S_LAUNCHXL/demos/trigger_mode/mqueue_nortos.c</path>
      <path copyTo="$PROJ_DIR$/trigger_mode.c">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/nortos/CC3220S_LAUNCHXL/demos/trigger_mode/trigger_mode.c</path>
      <path copyTo="$PROJ_DIR$/CC3220S_LAUNCHXL.c">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/nortos/CC3220S_LAUNCHXL/demos/trigger_mode/CC3220S_LAUNCHXL.c</path>
      <path copyTo="$PROJ_DIR$/$PROJ_FNAME$_CC3220S_LAUNCHXL_NoRTOS.icf">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/nortos/CC3220S_LAUNCHXL/demos/trigger_mode/iar/CC3220S_LAUNCHXL_NoRTOS.icf</path>
    </group>

    <group name="Header files">
      <path copyTo="$PROJ_DIR$/uart_term.h">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/nortos/CC3220S_LAUNCHXL/demos/trigger_mode/uart_term.h</path>
      <path copyTo="$PROJ_DIR$/mqueue.h">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/nortos/CC3220S_LAUNCHXL/demos/trigger_mode/mqueue.h</path>
      <path copyTo="$PROJ_DIR$/Board.h">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/nortos/CC3220S_LAUNCHXL/demos/trigger_mode/Board.h</path>
      <path copyTo="$PROJ_DIR$/CC3220S_LAUNCHXL.h">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/nortos/CC3220S_LAUNCHXL/demos/trigger_mode/CC3220S_LAUNCHXL.h</path>
    </group>

    <group name="Documentation" build="false">
      <path copyTo="$PROJ_DIR$/README.html">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/nortos/CC3220S_LAUNCHXL/demos/trigger_mode/README.html</path>
      <path copyTo="$PROJ_DIR$/README.md">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/nortos/CC3220S_LAUNCHXL/demos/trigger_mode/README.md</path>
      <path reference="$PROJ_DIR$/Board.html">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/boards/CC3220S_LAUNCHXL/Board.html</path>
    </group>
  </files>
</iarProjectConnection>
